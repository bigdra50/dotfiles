	    const graphData = {
	      labels: labels,
	      datasets: [{
	        label: '平均心拍数(bpm)',
	        data: hr_averages,
	        fill: false,
	      },
	      {
	        label: '最低心拍数(bpm)',
	        data: hr_lowest,
	        fill: false,
	      }]
	    };
	
	    const options = {
	      plugins: {
	        colorschemes: {
	          scheme: 'brewer.Paired12'
	        }
	      }
