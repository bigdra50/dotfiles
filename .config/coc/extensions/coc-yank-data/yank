	const model = {
	  Activity: {
	    labels: [],
	  },
	  Readiness: {
	    labels: [],
	  },
	  Sleep: {
	    labels: [],
	    hr_averages: [],
	    hr_lowers: [],
	  },
	};
	
	const update = (msg, json, model) => {
	  const nextModel = Object.assign({}, model);
	  switch (msg) {
	    case 'Activity':
	      console.log(json.activity);
	      nextModel[Activity].labels = json.activity.map(x => x.summary_date);
	      break;
	    case 'Readiness':
	      console.log(json.readiness);
	      nextModel[Readiness].labels = json.readiness.map(x => x.summary_date);
	      break;
	    case 'Sleep':
	      console.log(json.sleep);
	      nextModel[Sleep].labels = json.sleep.map(x => x.summary_date);
	      nextModel[Sleep].hr_averages = json.sleep.map(x => x.hr_averages);
	      nextModel[Sleep].hr_lowest = json.sleep.map(x => x.hr_lowers);
	      break;
	    default:
	      break;
	  }
	  return nextModel;
	}
	
	const URLJoin = (...args) =>
	  args.join('/')
	    .replace(/[\/]+/g, '/')
