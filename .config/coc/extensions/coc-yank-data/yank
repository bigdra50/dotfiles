	    const joinUrl = (...args) =>
	      args.join('/')
	        .replace(/[\/]+/g, '/')
	        .replace(/^(.+):\//, '$1://')
	        .replace(/^file:/, 'file:/')
	        .replace(/\/(\?|&|#[^!])/g, '$1')
	        .replace(/\?/g, '&')
	        .replace('&', '?');
	
	    const toYYYYMMDD = dt => {
	      const y = dt.getFullYear();
	      const m = ("00" + (dt.getMonth() + 1)).slice(-2);
	      const d = ("00" + dt.getDate()).slice(-2);
	      return y + "-" + m + "-" + d;
	    }
	
	    const getOuraData = type => {
	      const host = "https://api.ouraring.com/v1/";
	      const today = new Date();
	      const lastMonth = new Date();
	      lastMonth.setDate(today.getDate() - 30);
	      const startDate = "?start=" + toYYYYMMDD(lastMonth);
	      const endDate = "?end=" + toYYYYMMDD(new Date());
	      const accessToken = "?access_token=E7VCEPQMJKI73OMWZ5BYLLIILRVKTFNP";
	      switch (type) {
	        case 'activity':
	          return joinUrl(host, "activity", startDate, endDate, accessToken);
	        case 'readiness':
	          return joinUrl(host, "readiness", startDate, endDate, accessToken);
	        case 'sleep':
	          return joinUrl(host, "sleep", startDate, endDate, accessToken);
	        default:
	          break;
	      }
	    }
