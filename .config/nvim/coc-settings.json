{
  "suggest.enablePreview": true,
  "suggest.timeout": 5000,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.signOffset": 100,
  "diagnostic.warningSign": "--",
  "diagnostic.refreshOnInsertMode": true,
  "languageserver": {
    "elmLS": {
      "command": "~/.config/yarn/global/node_modules/.bin/elm-language-server",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "elm"
      ],
      "rootPatterns": [
        "elm.json"
      ],
      "trace.server": "verbose",
      "initializationOptions": {
        "runtime": "node",
        "elmPath": "~/.config/yarn/global/node_modules/.bin/elm",
        "elmFormatPath": "~/.config/yarn/global/node_modules/.bin/elm-format",
        "elmTestPath": "~/.config/yarn/global/node_modules/.bin/elm-test",
        "elmAnalyseTrigger": "change"
      }
    },
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    }
  },
  "codeLens.enable": true
}
